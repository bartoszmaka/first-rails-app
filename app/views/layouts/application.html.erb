<!DOCTYPE html>
<html>
  <head>
    <title>FromScratch</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class='container'>
      <h1><%= yield(:header) %></h1>
      <%= render 'shared/navigation'%>
      <%= yield %>
    </div>
    <%= debug(params) %>
  </body>
</html>
